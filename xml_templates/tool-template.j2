<?xml version="1.0" encoding="UTF-8"?>
<metadata xml:lang="en">
	<Esri>
		<CreaDate>{{create_date}}</CreaDate>
		<CreaTime>{{create_time}}</CreaTime>
		<ArcGISFormat>1.0</ArcGISFormat>
		<SyncOnce>TRUE</SyncOnce>
		<ModDate>{{modified_date}}</ModDate>
		<ModTime>{{modified_time}}</ModTime>
	</Esri>
	{% block tools %}{% endblock %}
	<dataIdInfo>
		{% block dataInfo %}{% endblock %}
		<idCredit>{{author}}</idCredit>
		<searchKeys>
			{% block keywords %}
			{% for keyword in keywords %}
			<keyword>{{keyword}}</keyword>
			{% endfor %}
			{% endblock %}
		</searchKeys>
		<resConst>
			<Consts>
				<useLimit>&lt;DIV STYLE="text-align:Left;"&gt;&lt;DIV&gt;&lt;P&gt;&lt;SPAN&gt;Licence: {{licence}}&lt;/SPAN&gt;&lt;/P&gt;&lt;/DIV&gt;&lt;/DIV&gt;</useLimit>
			</Consts>
		</resConst>
	</dataIdInfo>
	<distInfo>
		<distributor>
			<distorFormat>
				<formatName>ArcToolbox Tool</formatName>
			</distorFormat>
		</distributor>
	</distInfo>
</metadata>
